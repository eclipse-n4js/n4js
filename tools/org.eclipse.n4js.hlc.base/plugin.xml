<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="org.eclipse.n4js.hlc.base.N4jscBase">
         </run>
      </application>
   </extension>
   <extension
         id="org.eclipse.n4js.headless.product"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.n4js.hlc.base.application"
            name="N4JS Headless">
         <property
               name="appName"
               value="N4JS Headless">
         </property>
      </product>
   </extension>

</plugin>
