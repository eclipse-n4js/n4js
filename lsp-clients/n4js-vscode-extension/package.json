{
	"name": "n4js-vscode-extension",
	"displayName": "N4JS VSCode extension",
	"description": "Language support for N4JS",
	"version": "0.18.0",
	"publisher": "n4js",
	"license": "EPL-1.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/Eclipse/n4js"
	},
	"bugs": {
		"url": "https://github.com/Eclipse/n4js/issues"
	},
	"icon": "images/n4js_logo.png",
	"engines": {
		"vscode": "^1.41.1"
	},
	"categories": [
		"Programming Languages"
	],
	"activationEvents": [
		"onLanguage:n4js"
	],
	"main": "./extension",
	"contributes": {
		"languages": [
			{
				"id": "n4js",
				"aliases": [
					"n4js"
				],
				"extensions": [
					".n4js",
					".n4jsd",
					".n4jsx",
					".json",
					".n4idl"
				],
				"configuration": "./n4js.configuration.json"
			}
		],
		"grammars": [
			{
				"language": "n4js",
				"scopeName": "source.n4js",
				"path": "./syntaxes/n4js.tmLanguage.json"
			}
		],
		"commands": [
			{
				"command": "n4js.clean",
				"title": "N4JS: Clean"
			}
		],
		"activationEvents": ["onCommand:n4js.clean"],
		"menus": {
			"commandPalette": [
				{
					"command": "n4js.clean"
				}
			]
		}
	},
	"devDependencies": {
		"@types/vscode": "^1.41.0",
		"vsce": "1.71.0"
	},
	"dependencies": {
		"n4js-cli": "latest",
		"n4js-runtime": "latest",
		"n4js-runtime-node": "latest",
		"n4js-runtime-es2015": "latest",
		"npmlog": "4.1.2",
		"vscode-jsonrpc": "5.0.1",
		"vscode-languageclient": "6.1.0"
	},
	"scripts": {},
	"n4js": {
		"projectType": "library",
		"sources": {
			"source": [
				"src"
			]
		},
		"output": "src-gen",
		"requiredRuntimeLibraries": [
			"n4js-runtime-node",
			"n4js-runtime-es2015"
		]
	}
}
