import {AbstractN4jsdGeneratorTest} from "AbstractN4jsdGeneratorTest";

/**
 * Tests some simple conversions.
 */
export public class ConvertSimpleTest extends AbstractN4jsdGeneratorTest {

	@Test
	public testClass() {
		this.assertConversion([
			`
				// TestModule.d.ts
				export class Buffer {}
			`
		], [
			`
				// TestModule.n4jsd
				export external class ~Buffer {
				}
			`
		]);
	}

}
