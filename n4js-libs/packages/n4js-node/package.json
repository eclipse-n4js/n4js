{
  "name": "n4js-node",
  "version": "0.1.0",
  "author": {
    "name": "NumberFour AG",
    "email": "npm@numberfour.eu"
  },
  "license": "EPL-1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/eclipse/n4js"
  },
  "description": "N4JS node.js runtime environment.",
  "keywords": [
    "n4js",
    "es6",
    "es2015",
    "transpiler",
    "ide",
    "n4jside",
    "n4js-ide"
  ],
  "engines": {
    "node": ">= 5.0.0"
  },
  "bin": {
    "n4js": "./src-gen/n4js-cli.js"
  },
  "main": "./src-gen/index.js",
  "browser": "./src-gen/browser.js",
  "react-native": "./src-gen/react-native.js",
  "dependencies": {
    "esm": "~3.2.10",
    "node-fetch": "~2.2.0",
    "n4js-es5": "^0.1.0"
  },
  "files": [
    "src",
    "src-gen"
  ],
  "n4js": {
    "useES6Imports": true,
    "projectType": "runtimeEnvironment",
    "vendorId": "org.eclipse.n4js",
    "vendorName": "Eclipse N4JS Project",
    "output": "src-gen",
    "sources": {
      "external": [
        "src/js"
      ]
    },
    "moduleFilters": {
      "noModuleWrap": [
        "**"
      ]
    },
    "extendedRuntimeEnvironment": "n4js-es5",
    "providedRuntimeLibraries": [
      "n4js-runtime-n4",
      "n4js-runtime-v8",
      "n4js-runtime-es2015",
      "n4js-runtime-esnext",
      "n4js-runtime-fetch",
      "n4js-runtime-node",
      "n4js-runtime-ecma402"
    ],
    "moduleLoader": "n4js",
    "execModule": "n4js-cli"
  }
}
