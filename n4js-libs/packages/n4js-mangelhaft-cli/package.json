{
  "name": "n4js-mangelhaft-cli",
  "version": "0.1.0",
  "author": {
    "name": "NumberFour AG",
    "email": "npm@numberfour.eu"
  },
  "license": "EPL-1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/eclipse/n4js"
  },
  "description": "Node.js runner used to compile and launch the mangelhaft tests from command line.",
  "bin": {
    "n4js-mangelhaft": "./src-gen/org/eclipse/n4js/mangelhaft/runner/node/NodeTestMain.js"
  },
  "type": "module",
  "dependencies": {
    "@n4jsd/debug": "<=4.1.*",
    "@n4jsd/glob": "<=7.1.*",
    "cli-color": "~1.2.0",
    "debug": "~4.3.1",
    "glob": "^7.1.6",
    "istanbul-lib-coverage": "^3.2.0",
    "minimatch": "3.0.4",
    "n4js-runtime": "^0.1.0",
    "n4js-runtime-es2015": "^0.1.0",
    "n4js-runtime-esnext": "^0.1.0",
    "n4js-runtime-node": "^0.1.0",
    "node-fetch": "~2.6.1",
    "nomnom-patched": "~1.8.2",
    "org.eclipse.n4js.mangelhaft": "^0.1.0",
    "org.eclipse.n4js.mangelhaft.reporter.console": "^0.1.0",
    "org.eclipse.n4js.mangelhaft.reporter.xunit": "^0.1.0"
  },
  "n4js": {
    "projectType": "library",
    "vendorId": "org.eclipse.n4js",
    "vendorName": "Eclipse N4JS Project",
    "output": "src-gen",
    "sources": {
      "source": [
        "src/n4js"
      ]
    },
    "requiredRuntimeLibraries": [
      "n4js-runtime-es2015",
      "n4js-runtime-esnext",
      "n4js-runtime-node"
    ],
    "generator": {
      "rewriteCjsImports": true
    }
  }
}
