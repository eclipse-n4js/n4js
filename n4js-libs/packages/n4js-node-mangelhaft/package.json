{
  "name": "n4js-node-mangelhaft",
  "version": "0.1.0",
  "author": {
    "name": "NumberFour AG",
    "email": "npm@numberfour.eu"
  },
  "license": "EPL-1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/NumberFour/n4js-libs"
  },
  "description": "N4JS node.js mangelhaft runtime environment.",
  "keywords": [
    "n4js",
    "mangelhaft"
  ],
  "engines": {
    "node": ">= 5.0.0"
  },
  "dependencies": {
    "n4js-node": "^0.1.0",
    "n4js.lang": "^0.1.0",
    "org.eclipse.n4js.mangelhaft": "^0.1.0",
    "org.eclipse.n4js.mangelhaft.runner.ide": "^0.1.0",
    "org.eclipse.n4js.mangelhaft.reporter.ide": "^0.1.0"
  },
  "files": [
    "src",
    "src-gen"
  ],
  "n4js": {
    "useES6Imports": true,
    "projectType": "runtimeEnvironment",
    "vendorId": "org.eclipse.n4js",
    "vendorName": "Eclipse N4JS Project",
    "output": "src-gen",
    "sources": {
      "external": [
        "src/js"
      ]
    },
    "moduleFilters": {
      "noModuleWrap": [
        "**"
      ]
    },
    "extendedRuntimeEnvironment": "n4js-node",
    "providedRuntimeLibraries": [
      "n4js-runtime-n4",
      "n4js-runtime-v8",
      "n4js-runtime-es2015",
      "n4js-runtime-esnext",
      "n4js-runtime-fetch",
      "n4js-runtime-mangelhaft",
      "n4js-runtime-ecma402"
    ],
    "moduleLoader": "n4js",
    "execModule": "n4js-mangelhaft-cli"
  },
  "devDependencies": {}
}
