{
  "name": "n4js-runtime",
  "version": "0.1.0",
  "author": {
    "name": "NumberFour AG",
    "email": "npm@numberfour.eu"
  },
  "license": "EPL-1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/eclipse/n4js"
  },
  "description": "N4JS runtime environment.",
  "engines": {
    "node": ">=14"
  },
  "main": "./index-bundled.js",
  "types": "./n4jsglobals.d.ts",
  "files": [
    "src",
    "src-gen/n4js/lang/N4Injector.js",
    "index-bundled.js",
    "n4jsglobals.d.ts"
  ],
  "scripts": {
    "bundle": "./npm-bundle.sh"
  },
  "devDependencies": {
    "esbuild": "0.14.3"
  },
  "dependencies": {
    "n4js-runtime-es2015": "^0.1.0",
    "n4js-runtime-esnext": "^0.1.0"
  },
  "keywords": [
    "n4js",
    "es6",
    "es2015",
    "transpiler",
    "ide",
    "n4jside",
    "n4js-ide"
  ],
  "type": "module",
  "n4js": {
    "projectType": "library",
    "vendorId": "org.eclipse.n4js",
    "vendorName": "Eclipse N4JS Project",
    "output": "src-gen",
    "sources": {
      "source": [
        "src/n4js"
      ],
      "external": [
        "src/js"
      ]
    },
    "requiredRuntimeLibraries": [
      "n4js-runtime-es2015",
      "n4js-runtime-esnext"
    ]
  }
}
