{
  "name": "n4js-es5",
  "version": "0.1.0",
  "author": {
    "name": "NumberFour AG",
    "email": "npm@numberfour.eu"
  },
  "license": "EPL-1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/eclipse/n4js"
  },
  "description": "(Abstract) N4JS ES5 runtime environment.",
  "engines": {
    "node": ">= 5.0.0"
  },
  "main": "./src-gen/rt.js",
  "files": [
    "src",
    "src-gen"
  ],
  "n4js": {
    "useES6Imports": true,
    "projectType": "runtimeEnvironment",
    "vendorId": "org.eclipse.n4js",
    "vendorName": "Eclipse N4JS Project",
    "output": "src-gen",
    "sources": {
      "external": [
        "src/js"
      ]
    },

    "initModules": [
      "rt"
    ]
  }
}
