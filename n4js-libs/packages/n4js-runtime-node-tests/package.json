{
  "name": "n4js-runtime-node-tests",
  "version": "0.3.0",
  "author": {
    "name": "NumberFour AG",
    "email": "npm@numberfour.eu"
  },
  "license": "EPL-1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/NumberFour/n4js-libs"
  },
  "description": "N4JS node runtime tests.",
  "scripts": {
    "build": "./npm-build.sh"
  },
  "devDependencies": {
    "n4js-runtime-n4": "latest",
    "n4js-runtime-es2015": "latest",
    "n4js-runtime-node": "latest",
    "babel-cli": "^6.6.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.7.4",
    "babel-preset-es2015": "^6.6.0"
  },
  "dependencies": {
    "org.eclipse.n4js.mangelhaft.assert": "latest",
    "n4js-node": "latest"
  },
  "n4js_manifest": {
    "ProjectId": "n4js-runtime-node-tests",
    "VendorId": "org.eclipse.n4js",
    "VendorName": "Eclipse N4JS Project",
    "ProjectType": "test",
    "ProjectVersion": "0.3.0",
    "Output": "src-gen",
    "Sources": {
      "source": [
        "\"src/n4js\""
      ],
      "test": [
        "\"test/n4js\""
      ]
    },
    "ModuleFilters": {
      "noModuleWrap": [
        "\"**/run-es5\""
      ]
    },
    "TestedProjects": [
      "n4js-runtime-node"
    ],
    "ProjectDependencies": [
      "org.eclipse.n4js.mangelhaft.assert"
    ],
    "RequiredRuntimeLibraries": [
      "n4js-runtime-n4",
      "n4js-runtime-es2015",
      "n4js-runtime-node"
    ]
  }
}