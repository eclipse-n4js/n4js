import {RequireModuleTest} from "es-module-tests/RequireModuleTest";
/**
 * Handwritten ES5 run files being node.js executed.
 */
export public class RequireHandWrittenES5ModuleTest extends RequireModuleTest {
    @Override
    protected getRunnerModuleIdOf(exampleId: string): string {
