/* DestructuringAssignmentPadding1 XPECT astOrder ---
	0: function DestructuringAssignmentPadding1(x) { [\,\,x] = [1\,2\,3]; }\n,
	1: { [\,\,x] = [1\,2\,3]; },
	2: [\,\,x] = [1\,2\,3];,
	3: [\,\,x] = [1\,2\,3],
	4: [\,\,x],
	5: x,
	6: [1\,2\,3],
	7: 1,
	8: 2,
	9: 3
--- */
function DestructuringAssignmentPadding1(x) {
	[,,x] = [1,2,3];
}
