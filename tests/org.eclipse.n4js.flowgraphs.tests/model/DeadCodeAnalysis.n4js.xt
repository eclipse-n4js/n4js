/* XPECT_SETUP org.eclipse.n4js.flowgraphs.tests.N4JSFlowgraphTest END_SETUP */


function f1() {
	/* Forward XPECT allPaths ---
		"start" -> 1 -> "x" -> throw "x"; -> 3 -> 6
	---	*/
	/* Backward XPECT allPaths direction 'Backward' ---
		"end" -> 6 -> 4 -> 3 -> 2,
		6 -> 3 -> throw "x"; -> "x" -> 1 -> "start"
	---	*/
	"start";
	try {
		try {
			1;
			throw "x";
			// XPECT warnings --> "Dead code. No execution possible after throw." at "2"
			2;
		} finally {
			3;
		}
		// XPECT warnings --> "Dead code. No execution possible after TryStatement." at "4"
		4;
	} finally {
		6;
	}
	// XPECT warnings --> "Dead code. No execution possible after TryStatement." at ""end""
	"end";
}



function f2() {
	// f2 XPECT allIslandElems --> 1, 2, 3, 4
	return;
	//  XPECT warnings --> "Dead code." at "1"
	for (;;)  1;
	//  XPECT warnings --> "Dead code. No execution possible after ForStatement." at "2; 3; for (;;)  4"
	2;

	3;

	for (;;)  4;
}

function f3() {
	"start";
	try {
		1;
	} catch (e) {
		try {
			2;
			throw 3;
		} catch (ee) {
			4;
		}
		5;
	}
	"end";
}

function f4() {
	/*  XPECT allEdges ---
		"start" --> 1,
		0 --> 4,
		1 --> 4,
		2 --> 3,
		3 --> throw 3;,
		4 --> "end",
		throw 3; -Throw-> 4
	--- */
	/* Forward X-PECT allPaths ---
		"start" -> 1 -> "x" -> throw "x"; -> 3 -> 6,
	---	*/
	/* ThrowTryCatchFinallyStatement2 XPECT allPaths direction 'Backward' ---
		"end" -> 4 -> 1 -> "start",
		"end" -> 4 -> 0,
		4 -> throw 3; -> 3 -> 2
	---	*/
	"start";
	try {
		1;

	} catch (e) {
		2;
		throw 3;
		// XPECT warnings --> "Dead code. No execution possible after throw." at "0"
		0;

	} finally {
		4;
	}
	"end";
}
