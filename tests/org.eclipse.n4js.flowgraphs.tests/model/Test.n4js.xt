/* XPECT_SETUP org.eclipse.n4js.flowgraphs.tests.N4JSFlowGraphTest END_SETUP */



function ReturnStatement2() {
	/* XPECT allPaths ---
		"start" -> 1 -> 2 -> 3 -> "finally" -> "end",
		"start" -> 1 -> 2 -> return; -> "finally"
	--- */
	"start";
	try {
		1;
		if (2)
			return;
		3;
	} finally {
		"finally";
	}
	// XPECT nowarnings --> "Dead code. No execution possible after try statement." at ""end";"
	"end";
}
