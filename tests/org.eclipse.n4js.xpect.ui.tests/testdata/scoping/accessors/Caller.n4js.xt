import * as N from "scoping/accessors/Callee"

class Caller {

	call() {
		var callee : N.Callee = null;

// XPECT scope at 'callee.<|>a' --> a, !b, c, d, data_property_b, !data_property_c, !newB, ...
		callee.a = "newA"
// XPECT scope at 'callee.<|>a' --> a, b, !c, d, data_property_b, !data_property_c, !newB, ...
		var newA = callee.a

		var newB = callee.b

		callee.c = "newC"

		callee.d = "newD"
		var newD = callee.d
	}
}
