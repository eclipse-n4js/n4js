export class Callee {
	project a : string;
	private data_property_b : string;
	private data_property_c : string;
	private data_property_d : string;
	
	project get b() : string {
		return this.data_property_b + "_getter";
	}
	
	project set c(newC : string) {
		this.data_property_c = newC + "_setter";
	}

	project get d() : string {
		return this.data_property_d + "_getter";
	}

	project set d(newD : string) {
		this.data_property_d = newD + "_setter";
	}
}
