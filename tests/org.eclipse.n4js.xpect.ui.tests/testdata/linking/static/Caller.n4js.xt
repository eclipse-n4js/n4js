import { Callee } from "linking/static/Callee"
// XPECT warnings --> "The import of * as N from linking/static/SubCallee2 is unused." at "* as N"
import * as N from "linking/static/SubCallee2"

class Caller {

    call() {
		// XPECT linkedFragment at 'Callee.|myStaticField' --> Callee.n4js#/1/@topLevelTypes.0/@ownedMembers.1
		var a = Callee.myStaticField

		// XPECT linkedFragment at 'Callee.|myStaticMethod' --> Callee.n4js#/1/@topLevelTypes.0/@ownedMembers.3
		Callee.myStaticMethod

		// XPECT linkedFragment at 'Callee.|myPrivateStaticAccessor' --> Callee.n4js#/1/@topLevelTypes.0/@ownedMembers.5
		a = Callee.myPrivateStaticAccessor

		// XPECT linkedFragment at 'Callee.|myPrivateStaticAccessor' --> Callee.n4js#/1/@topLevelTypes.0/@ownedMembers.6
		Callee.myPrivateStaticAccessor = a

		var callee : Callee = new Callee

		// XPECT linkedFragment at 'callee.|myNonStaticField' --> Callee.n4js#/1/@topLevelTypes.0/@ownedMembers.0
		callee.myNonStaticField

		// XPECT linkedFragment at 'callee.|myNonStaticMethod' --> Callee.n4js#/1/@topLevelTypes.0/@ownedMembers.4
		callee.myNonStaticMethod

		// XPECT linkedFragment at 'callee.|myRoleNonStaticField' --> CalleeTrait.n4js#/1/@topLevelTypes.0/@ownedMembers.0
		callee.myRoleNonStaticField

		// XPECT linkedFragment at 'callee.|myRoleNonStaticMethod' --> CalleeTrait.n4js#/1/@topLevelTypes.0/@ownedMembers.1
		callee.myRoleNonStaticMethod

		// XPECT ! linkedFragment at 'constructor.|myStaticField' -->
		callee.constructor.myStaticField

		// XPECT ! linkedFragment at 'Callee.|myStaticMethod' -->
		callee.constructor.myStaticMethod

		// XPECT ! linkedFragment at 'Callee.|myPrivateStaticAccessor' -->
		a = callee.constructor.myPrivateStaticAccessor

		// XPECT ! linkedFragment at 'Callee.|myPrivateStaticAccessor' -->
		Callee.myPrivateStaticAccessor = a
	}
}
