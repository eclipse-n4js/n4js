// should work with a custom iterable:

class MyCollection implements Iterable<number> {
	@Override
	public [Symbol.iterator]() : Iterator<number>{
		return null;
	}
}
var myColl = new MyCollection();
// XPECT noerrors -->
for(elem of myColl) {}


