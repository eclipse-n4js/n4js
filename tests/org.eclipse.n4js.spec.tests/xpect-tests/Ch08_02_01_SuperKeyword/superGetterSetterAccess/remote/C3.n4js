import { C2 } from "Ch08_02_01_SuperKeyword/superGetterSetterAccess/remote/C2"


export public class C3 extends C2 {
	test() {
		console.log('* testing access with "this"');
		this.g;
		this.s = 42;
		this.m();
		this.gI;
		this.sI = 42;
		this.mI();
		console.log('* testing access with "super"');
		super.g;
		super.s = 42;
		super.m();
		super.gI;
		super.sI = 42;
		super.mI();
	}
}

