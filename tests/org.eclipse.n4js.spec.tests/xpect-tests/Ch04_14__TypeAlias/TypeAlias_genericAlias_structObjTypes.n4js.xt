/* XPECT_SETUP org.eclipse.n4js.spec.tests.N4JSSpecTest END_SETUP */


class A {}
class B extends A {}
class C extends B {}

let oa: ~Object with { m(A) };
let ob: ~Object with { m(B) };
let oc: ~Object with { m(C) };

type AO<T> = ~Object with { m(T) };

let aoa: AO<A>;
let aob: AO<B>;
let aoc: AO<C>;

// XPECT noerrors -->
aob = oa;
// XPECT noerrors -->
aob = ob;
// XPECT errors --> "~Object with { m(_par_0: C): void } is not a structural subtype of ~Object with { m(_par_0: T): void } [[T->B]]: m failed: {function(C):void} is not a subtype of {function(B):void}." at "oc"
aob = oc;

// XPECT noerrors -->
ob = aoa;
// XPECT noerrors -->
ob = aob;
// XPECT errors --> "~Object with { m(_par_0: T): void } [[T->C]] is not a structural subtype of ~Object with { m(_par_0: B): void }: m failed: {function(C):void} is not a subtype of {function(B):void}." at "aoc"
ob = aoc;
