/* XPECT_SETUP org.eclipse.n4js.spec.tests.N4JSSpecTest END_SETUP */


class A {}
class B {}

class G<T1, T2> {
	m1(): T1 { return null; }
	m2(): T2 { return null; }
}
class H<T> extends G<A, T> {}

type AH<T> = H<T>;

let h: H<B>;
let ah: AH<B>;

// XPECT type of 'h.m1()' --> A
h.m1();
// XPECT type of 'h.m2()' --> B
h.m2();

// XPECT type of 'ah.m1()' --> A
ah.m1();
// XPECT type of 'ah.m2()' --> B
ah.m2();
