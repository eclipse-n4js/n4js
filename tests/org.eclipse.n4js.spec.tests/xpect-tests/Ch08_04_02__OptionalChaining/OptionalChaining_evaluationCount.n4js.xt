/* XPECT_SETUP org.eclipse.n4js.spec.tests.N4JSSpecTest END_SETUP */


class G {

	@Final public g: ()=>G;
	@Final public x: ()=>G;
	@Final public u: ()=>G;

	constructor(id: string, nextG: G) {
		const correctThis = this;

		this.g = function() {
			console.log(id);
			if (this !== correctThis) console.log("FAILURE: incorrect call context!");
			return nextG;
		};

		this.x = function() {
			console.log(id);
			if (this !== correctThis) console.log("FAILURE: incorrect call context!");
			return null;
		};

		this.u = undefined;
	}
}

let g = new G("t0", new G("t1", new G("t2", new G("t3", null))));


g.g().g()?.g().g();
console.log("======");
g.g().x()?.g().g();
console.log("======");
g.g().g().g?.().g();
console.log("======");
g.g().g().u?.().g();
console.log("======");
g?.g?.()?.g?.()?.g?.()?.g?.();
console.log("======");
g?.g?.()?.g?.()?.u?.()?.g?.();


