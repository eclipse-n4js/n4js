/* XPECT_SETUP org.eclipse.n4js.spec.tests.N4JSSpecTest END_SETUP */

interface I {
	f() { console.log("I.f"); }
}

// XPECT noerrors --> "f is consumed"
class A implements I {}

// XPECT noerrors --> No error expected: "f is inherited"
class B extends A implements I{
}


/* XPECT output ---
I.f
I.f
--- */


var a = new A();
var b = new B();



// XPECT binding --> I.f
a.f();

// XPECT binding --> I.f
b.f();
