/* XPECT_SETUP org.eclipse.n4js.spec.tests.N4JSSpecTest END_SETUP */

// independent types
interface X {}
interface Y {}


class A {
	m(p : X) : Y { return null }
}

interface I {
	m(p : Y) : X
}

// XPECT noerrors --> "conflict is solved in owned method"
class B extends A implements I {

	@Override
	m(p : union{X,Y}) : intersection{X,Y} { console.log("B.m()"); return null; }

}


/* XPECT output ---
B.m()
--- */

// XPECT binding --> B.m
new B().m(null);
