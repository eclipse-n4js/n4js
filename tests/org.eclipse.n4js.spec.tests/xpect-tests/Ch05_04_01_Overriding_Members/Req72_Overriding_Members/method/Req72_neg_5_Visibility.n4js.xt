/* XPECT_SETUP org.eclipse.n4js.spec.tests.N4JSSpecTest END_SETUP */



class A {
	public f(){ console.log("A.f()"); }
}

class B extends A {
	@Override
	// XPECT errors --> "The method B.f cannot reduce the visibility of method A.f." at "f"
	f(){ console.log("B.f()"); }
}

var a = new A();
var b : B = new B();

// XPECT binding --> A.f
a.f();
// XPECT binding -->       B.f
b.f();
