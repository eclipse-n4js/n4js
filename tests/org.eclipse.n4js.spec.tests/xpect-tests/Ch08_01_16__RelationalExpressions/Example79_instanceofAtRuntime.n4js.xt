// IDE-561

/* XPECT output ---
A is instance of I
B is instance of I
D is instance of R
E is instance of R
F is instance of I
F is instance of R
--- */

interface I {}
interface R {}

class A implements I {}
class B extends A {}
class C {}
class D implements R {}
class E extends D {}
class F extends B implements R {}

function f(name : string, p : any) {
	if (p instanceof I) {
		console.log(name + " is instance of I");
	}
	if (p instanceof R) {
		console.log(name + " is instance of R");
	}
}

f("A", new A())
f("B", new B())
f("C", new C())
f("D", new D())
f("E", new E())
f("F", new F())
