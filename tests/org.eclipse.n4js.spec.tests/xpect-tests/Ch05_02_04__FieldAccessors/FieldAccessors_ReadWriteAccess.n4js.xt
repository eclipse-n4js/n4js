/* XPECT_SETUP org.eclipse.n4js.spec.tests.N4JSSpecTest END_SETUP */


//
// with class
//

class C {
	get g() : string { return null; }
	set s(p : string) {}
}

var c : C;

c.g;		// ok
// XPECT errors --> "The accessor g is read-only." at "g"
c.g = "";	// nok

// XPECT errors --> "The accessor s is write-only." at "s"
c.s;		// nok
c.s = "";	// ok



//
// with object literal
//

var ol = {
	get g() : string { return null; },
	set s(p : string) {}
};

ol.g;		// ok
// XPECT errors --> "The accessor g is read-only." at "g"
ol.g = "";	// nok

// XPECT errors --> "The accessor s is write-only." at "s"
ol.s;		// nok
ol.s = "";	// ok



//
// with structural typing
//

var st : ~Object with {
		get g() : string
		set s(p : string)
	};

st.g;		// ok
// XPECT errors --> "The accessor g is read-only." at "g"
st.g = "";	// nok

// XPECT errors --> "The accessor s is write-only." at "s"
st.s;		// nok
st.s = "";	// ok
