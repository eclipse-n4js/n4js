/* XPECT_SETUP org.eclipse.n4js.spec.tests.N4JSSpecTest END_SETUP  */


console.log('####### class/interface');

interface I {
	// XPECT noerrors -->
	public ['field']: string;
	// XPECT noerrors -->
	get ['acc'](): string
	// XPECT noerrors -->
	set ['acc'](value: string)
	// XPECT noerrors -->
	['method']()
}
class C implements I {
	private value: string = 'hello from private field';
	// XPECT noerrors -->
	@Override public ['field']: string = 'hello from public field';
	// XPECT noerrors -->
	@Override get ['acc'](): string { return this.value; }
	// XPECT noerrors -->
	@Override set ['acc'](value: string) { this.value = value; }
	// XPECT noerrors -->
	@Override ['method']() { console.log('hello from method'); }
}

let i: I = new C();
console.log(i.field);
console.log(i.acc);
i.acc = 'hello from private field (new value!)';
console.log(i.acc);
i.method();


console.log('####### object literal');

let ol = {
	value: 'hello from private property',
	// XPECT noerrors -->
	['field']: 'hello from public property',
	// XPECT noerrors -->
	get ['acc'](): string { return this.value; },
	// XPECT noerrors -->
	set ['acc'](value: string) { this.value = value; },
	// XPECT noerrors -->
	['method']() { console.log('hello from method'); }
}

console.log(ol.field);
console.log(ol.acc);
ol.acc = 'hello from private property (new value!)';
console.log(ol.acc);
ol.method();


