var a : Array<string>;
// XPECT noerrors --> "index access on array ok"
var first : string = a[0];
// XPECT noerrors -->
a['length'];
// XPECT errors --> "Member hello not found." at "a['hello']"
a['hello'];

function f() {
	// XPECT noerrors --> "index access on array ok"
	arguments[0];
	// XPECT noerrors -->
	arguments['length'];
	// XPECT errors --> "Member hello not found." at "arguments['hello']"
	arguments['hello'];
}
