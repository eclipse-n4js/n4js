import { A } from "Ch11_02__DependencyInjection/DI_FieldAccessInCtorForSeparateModules_SuperClass"
import { B } from "Ch11_02__DependencyInjection/DI_FieldAccessInCtorForSeparateModules_SuperClass"

class C extends A {
	constructor() {
		// The DI related validation error is intentionally ignored. Field is not visible anyway.
		// XPECT errors --> "The field b1 is not visible." at "b1"
		this.b1.x = "x1";

		// XPECT noerrors --> "Field write access is intentionally ignored now."
    	this.b2 = null;

		// XPECT errors --> "b2 is not yet injected at this point." at "this.b2"
    	var bb : any = this.b2;

		// XPECT errors --> "b2 is not yet injected at this point." at "this.b2"
    	this.b2;

		// XPECT errors --> "b2 is not yet injected at this point." at "this.b2"
	    this.b2.x = "bar";

		// XPECT errors --> "b2 is not yet injected at this point." at "this.b2"
	    if (this.b2) {
	    	console.log('bar')
	    }

		// XPECT errors --> "b2 is not yet injected at this point." at "this.b2"
	    switch (this.b2) {
	    	default: console.log('bar');
	    }

		// XPECT errors --> "b2 is not yet injected at this point." at "this.b2"
	    var arr : Array<B> = [this.b2, null];

		// XPECT errors --> "b2 is not yet injected at this point." at "this.b2"
	    this.foo(this.b2)

    }

    foo(b : B) { }
}
