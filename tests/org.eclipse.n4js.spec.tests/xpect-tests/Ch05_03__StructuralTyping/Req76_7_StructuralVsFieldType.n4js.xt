class A {
	public n : number;
	public foo () : void {};
}

var f : ~~A; //  = { n: 10 }
var s : ~A;
// XPECT errors --> "~~A is not a structural subtype of ~A: missing method foo." at "f"
s = f;

// XPECT noerrors --> "~A <: ~~A always"
f = s;



class B {
	public n : number;
}

var fb : ~~B = { n: 10 }
var sb : ~B;
// XPECT noerrors --> "~~B <: ~B as B has no methods" at "fb"
sb = fb;
