/* XPECT_SETUP org.eclipse.n4js.spec.tests.N4JSSpecTest END_SETUP */


class C {}
class G<T> {
	field: T;
	fieldDyn: T+;

	m(p: T): T {return null;}
	mDyn(p: T+): T+ {return null;}
}


var g: G<C>;
var gDyn: G<C+>;


// part #1: reading

// XPECT type of 'v01' --> C
var v01 = g.field;
// XPECT type of 'v02' --> C+
var v02 = g.fieldDyn;
// XPECT type of 'v03' --> C
var v03 = g.m(undefined);
// XPECT type of 'v04' --> C+
var v04 = g.mDyn(undefined);

// XPECT type of 'v11' --> C+
var v11 = gDyn.field;
// XPECT type of 'v12' --> C+
var v12 = gDyn.fieldDyn;
// XPECT type of 'v13' --> C+
var v13 = gDyn.m(undefined);
// XPECT type of 'v14' --> C+
var v14 = gDyn.mDyn(undefined);


// part #2: writing

// XPECT errors --> "string is not a subtype of C." at """"
g.field = "";
// XPECT errors --> "string is not a subtype of C+." at """"
g.fieldDyn = "";
// XPECT errors --> "string is not a subtype of C." at """"
g.m("");
// XPECT errors --> "string is not a subtype of C+." at """"
g.mDyn("");

// XPECT errors --> "string is not a subtype of C+." at """"
gDyn.field = "";
// XPECT errors --> "string is not a subtype of C+." at """"
gDyn.fieldDyn = "";
// XPECT errors --> "string is not a subtype of C+." at """"
gDyn.m("");
// XPECT errors --> "string is not a subtype of C+." at """"
gDyn.mDyn("");
