/* XPECT_SETUP org.eclipse.n4js.spec.tests.N4JSSpecTest END_SETUP */

interface A {}
interface B {}

let a:A;
let b:B;

let aub:A|B;

interface I1<T> {
	m1:T;
	get m2(): T;
}

interface I2<T> {
	get m1(): T;
	m2: T;
}

interface I3<T> {
	get m1(): T;
	m2: T;
}

var i12: I1<A> & I2<B>;

// set m1(a:A|B)
// get m1():A&B

// XPECT noerrors
i12.m1;

// XPECT type of 'i12.m1' --> intersection{A,B}
i12.m1;

// XPECT noerrors
i12.m1 = a;


// set m2(a:A)
// get m2():A&B

// XPECT noerrors
i12.m2;

// XPECT type of 'i12.m2' --> intersection{A,B}
i12.m2;

// XPECT noerrors
i12.m2 = b;

var i13: I1<A> & I3<B>;

// set m1(a:A)
// get m1():A&B

// XPECT noerrors
i13.m1;

// XPECT type of 'i13.m1' --> intersection{A,B}
i13.m1;

// XPECT noerrors
i13.m1 = a;


// set m2(b:A)
// get m2():A&B

// XPECT noerrors
i13.m2;

// XPECT type of 'i13.m2' --> intersection{A,B}
i13.m2;

// XPECT noerrors
i13.m2 = b;


var i23: I2<A> & I3<B>;

// get m1():A&B

// XPECT noerrors
i23.m1;

// XPECT type of 'i23.m1' --> intersection{A,B}
i23.m1;

// XPECT errors --> "The accessor m1 is read-only." at "m1"
i23.m1 = null;

// set m2(aub:A|B)
// get m2():A&B

// XPECT noerrors
i23.m2;

// XPECT type of 'i23.m2' --> intersection{A,B}
i23.m2;

// XPECT noerrors
i23.m2 = aub;
