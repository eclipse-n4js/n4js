/* XPECT_SETUP org.eclipse.n4js.spec.tests.N4JSSpecTest END_SETUP */

interface A {}
interface B {}

let a:A;
let b:B;

let ab:A|B;

interface I1 {
	get a():A;
	set b(b:B);

	get ab():A;
	set ba(a:A);
}

interface I2 {
	set a(a:A);
	get b():B;

	set ab(b:B);
	get ba():B;
}

var i12: I1&I2;

// get a():A
// set a(a:A)

// XPECT noerrors
i12.a

// XPECT type of 'i12.a' --> A
i12.a

// XPECT noerrors
i12.a = a;

// XPECT errors --> "B is not a subtype of A." at "b"
i12.a = b;



// get b():B
// set b(b:B)

// XPECT noerrors
i12.b

// XPECT type of 'i12.b' --> B
i12.b

// XPECT noerrors
i12.b = b;

// XPECT errors --> "A is not a subtype of B." at "a"
i12.b = a;


// get ab():A
// set ab(b:B)

// XPECT noerrors
i12.ab

// XPECT type of 'i12.ab' --> A
i12.ab

// XPECT errors --> "A is not a subtype of B." at "a"
i12.ab = a;

// XPECT noerrors
i12.ab = b;


// get ba():B
// set ba(a:A)

// XPECT noerrors
i12.ba

// XPECT type of 'i12.ba' --> B
i12.ba

// XPECT noerrors
i12.ba = a;

// XPECT errors --> "B is not a subtype of A." at "b"
i12.ba = b;
