/* XPECT_SETUP org.eclipse.n4js.spec.tests.N4JSSpecTest END_SETUP */

interface A {}
interface B {}

let a:A;
let b:B;

let ab:A|B;

interface I1 {
	get onlyIn1(): A;
	get inBothSameType(): A;
	get inBothDiffType(): A;
}

interface I2 {
	get onlyIn2(): B;
	get inBothSameType(): A;
	get inBothDiffType(): B;
}

var i12 : I1&I2;
// get onlyIn1(): A
// get onlyin2(): B;
// get inBothSameType(): A
// get inBothDiffType(): A&B

// XPECT noerrors
i12.onlyIn1;

// XPECT type of 'i12.onlyIn1' --> A
i12.onlyIn1;

// XPECT errors --> "The accessor onlyIn1 is read-only in I1." at "onlyIn1"
i12.onlyIn1 = a;

// XPECT noerrors
i12.onlyIn2;

// XPECT type of 'i12.onlyIn2' --> B
i12.onlyIn2;

// XPECT errors --> "The accessor onlyIn2 is read-only in I2." at "onlyIn2"
i12.onlyIn2 = b;

// XPECT noerrors
i12.inBothSameType;

// XPECT type of 'i12.inBothSameType' --> A
i12.inBothSameType;

// XPECT errors --> "The accessor inBothSameType is read-only." at "inBothSameType"
i12.inBothSameType = a;

// XPECT noerrors
i12.inBothDiffType;

// XPECT type of 'i12.inBothDiffType' --> intersection{A,B}
i12.inBothDiffType;

// XPECT errors --> "The accessor inBothDiffType is read-only." at "inBothDiffType"
i12.inBothDiffType = ab;
