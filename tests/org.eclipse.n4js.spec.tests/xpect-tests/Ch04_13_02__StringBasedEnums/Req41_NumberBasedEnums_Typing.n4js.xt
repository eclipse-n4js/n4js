/* XPECT_SETUP org.eclipse.n4js.spec.tests.N4JSSpecTest END_SETUP */

@NumberBased enum E { L: 42 }

// XPECT noerrors -->
var s : number = E.L;

// Not a regular enum. XPECT errors --> "E is not a subtype of N4Enum." at "E.L"
var v1 : N4Enum = E.L;

// XPECT noerrors -->
var v2 : E = E.L;

// XPECT noerrors -->
var v3a : Number = E.L;

// XPECT noerrors -->
var v3b : number = E.L;

// XPECT noerrors -->
var v3c : int = E.L;

// XPECT noerrors -->
var v4 : N4NumberBasedEnum = E.L;


// XPECT noerrors -->
var x : E = E.L;


function x1 (arg : E){}

// XPECT noerrors -->
x1(x);
// XPECT noerrors -->
x1(E.L)
// This would be an uncheck use, that we want't to prevent.
// XPECT errors --> "int is not a subtype of E." at "42"
x1(42)
// XPECT errors --> "number is not a subtype of E." at "42.0"
x1(42.0)

// XPECT noerrors -->
function x2 (arg : number){}
// XPECT noerrors -->
x2(x);
// XPECT noerrors -->
x2(E.L)
// XPECT noerrors -->
x2(42)

// XPECT noerrors -->
function x3 () : E { return E.L; }

// Prohibit unchecked use XPECT errors --> "int is not a subtype of E." at "42"
function x4 () : E { return 42 }

// XPECT noerrors -->
function x5 () : E { var e = E.L; return e; }
