/* XPECT_SETUP org.eclipse.n4js.spec.tests.N4JSSpecTest END_SETUP */



class A {
	f(a : A){ console.log("A.f()"); }
}

abstract class B extends A {
	@Override
	// XPECT errors --> "The abstract method B.f cannot override concrete method A.f." at "f"
	abstract f(a : A)
}

var a = new A();
var b : B;

// XPECT binding --> A.f
a.f(null);
// XPECT binding -->       B.f
b.f(null);
