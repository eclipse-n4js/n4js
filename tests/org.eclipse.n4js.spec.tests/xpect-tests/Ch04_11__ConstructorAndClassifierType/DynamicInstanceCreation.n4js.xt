class A {
	constructor() {	console.log("A.ctor") }
	f() { console.log("A.f") }
}
class B extends A {
	constructor() {	console.log("B.ctor") }
	@Override f() { console.log("B.f") }
}
function dynamic(ctor : constructor{A}) {
	console.log("dynamic")
	var a : A = new ctor();
	a.f();
}
console.log("static")
new A().f();
dynamic(A);

console.log("static")
new B().f();
dynamic(B);
