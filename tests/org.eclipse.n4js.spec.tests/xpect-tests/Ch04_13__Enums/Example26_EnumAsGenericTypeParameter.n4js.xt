enum Color { R, G, B}

class Container<T extends N4Enum> {
	slot : T;

	add(t : T) { this.slot = t;}
 	get() : T { return this.slot }
}

var colors : Container<Color> = new Container<Color>();
colors.add(Color.R);

console.log(colors.get());
