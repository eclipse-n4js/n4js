/* XPECT_SETUP org.eclipse.n4js.spec.tests.SpecXtTest

	File "other.n4js" {

		var variableVar: string = "hello var";
		let variableLet: string = "hello let";
		const variableConst: string = "hello const";
		interface Ifc { method(p: string): int }
		class Cls { field: string }
		enum Enum { Li1, Lit2, Lit3 }
		function foo(p: string): int {}
		type TypeAlias = string;

		export { variableVar, variableLet, variableConst };
		export { Ifc, Cls, Enum };
		export { foo };
		export { TypeAlias };

	}

	// note: this test is testing internal infrastructure for a feature not available in N4JS[D] (only in .d.ts)
	IssueConfiguration {
		IssueCode "UNSUPPORTED" { enabled = false }
		IssueCode "VIS_ILLEGAL_FUN_ACCESS" { enabled = false }
		IssueCode "VIS_ILLEGAL_TYPE_ACCESS" { enabled = false }
		IssueCode "VIS_ILLEGAL_VARIABLE_ACCESS" { enabled = false }
		IssueCode "VIS_ILLEGAL_MEMBER_ACCESS" { enabled = false }
		IssueCode "VIS_NEW_CANNOT_INSTANTIATE_INVISIBLE_CONSTRUCTOR" { enabled = false }
	}

	END_SETUP
 */

// XPECT noerrors -->
import { variableVar, variableLet, variableConst } from "other"
// XPECT noerrors -->
import { Ifc, Cls, Enum } from "other"
// XPECT noerrors -->
import { foo } from "other"
// XPECT noerrors -->
import { TypeAlias } from "other"


// XPECT type of 'variableVar' --> string
variableVar;
// XPECT type of 'variableLet' --> string
variableLet;
// XPECT type of 'variableConst' --> string
variableConst;


let ifc: Ifc;
// XPECT type of 'v01' --> int
let v01 = ifc.method("");
// XPECT type of 'v02' --> string
let v02 = new Cls().field;
// XPECT type of 'Lit2' --> Enum.Lit2
Enum.Lit2;


// XPECT type of 'v03' --> int
let v03 = foo("hello");


// XPECT noerrors -->
let v04: TypeAlias = "hello";
// XPECT noerrors -->
let v05: string = v04;
