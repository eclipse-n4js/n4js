/* XPECT_SETUP org.eclipse.n4js.spec.tests.SpecXtTest END_SETUP  */


class A {}
class B extends A {}
class C extends B {}


var ia: Iterable<A>;
var ib: Iterable<B>;
var ic: Iterable<C>;

// XPECT errors --> "Iterable<A> is not a structural subtype of Iterable<B> due to incompatible type arguments: A is not a subtype of B." at "ia"
ib = ia;
// XPECT noerrors -->
ib = ic;



var is: Iterable<string>;
var iand: Iterable<int&string>;
var ior: Iterable<int|string>;

// XPECT errors --> "Iterable<union{int,string}> is not a structural subtype of Iterable<string> due to incompatible type arguments: union{int,string} is not a subtype of string." at "ior"
is = ior;
// XPECT noerrors -->
is = iand;


// XPECT noerrors -->
ior = is;

// XPECT errors --> "Iterable<string> is not a structural subtype of Iterable<intersection{int,string}> due to incompatible type arguments: string is not a subtype of intersection{int,string}." at "is"
iand = is;


var ieb: IteratorEntry<B>;

// XPECT errors --> "~Object with { done: boolean; value: A } is not a structural subtype of IteratorEntry<B>: value failed: A is not a subtype of B." at "{done: false, value: new A()}"
ieb = {done: false, value: new A()};
// XPECT noerrors -->
ieb = {done: false, value: new B()};
// XPECT noerrors -->
ieb = {done: false, value: new C()};
// XPECT noerrors -->
ieb = {done: true, value: undefined};
// XPECT noerrors -->
ieb = {done: true, value: null};
// XPECT noerrors -->
ieb = {done: true};
