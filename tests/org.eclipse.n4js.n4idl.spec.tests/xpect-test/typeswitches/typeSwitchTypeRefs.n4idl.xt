/* XPECT_SETUP org.eclipse.n4js.n4idl.spec.tests.N4IDLSpecTest END_SETUP */

class A#1 {}
class A#2 {}
class A#3 {}
class A#4 {}
class A#5 {}

class B#1<T> {}
class B#2<T> {}
class B#3<T> {}

interface I#1 {}
interface I#2 {}

interface J#1<T> {}
interface J#2<T1, T2> {}

@VersionAware
function f() {
	// Unparameterized type ref 1 XPECT typeSwitchTypeRef of a1 --> "A#1"
	let a1 : A#1
	// Unparameterized type ref 1 XPECT typeSwitchTypeRef of a2 --> "A#2"
	let a2 : A#2
	
	// Parameterized type ref 1 XPECT typeSwitchTypeRef of a3 --> "B#1"
	let a3 : B#1<A#1>
	// Parameterized type ref 2 XPECT typeSwitchTypeRef of a4 --> "Iterable2<?,?>"
	let a4 : Iterable2<A#1, A#1>
	
	// Parameterized array type ref 1 XPECT typeSwitchTypeRef of a5 --> "Array<A#1>"
	let a5 : Array<A#1>
	// Parameterized array type ref 2 ([<typeRef>] syntax) XPECT typeSwitchTypeRef of a6 --> "Array<A#1>"
	let a6 : [A#1]
	
	// Unparameterized composed type ref 1 XPECT typeSwitchTypeRef of a7 --> "union{intersection{union{Array<A#2>,Array<A#1>},union{intersection{I#1,A#3},A#5}},I#1}"
	let a7 : (([A#2|A#1])&((I#1&A#3|A#5)))|I#1
	
	// Parameterized composed type ref 1 XPECT typeSwitchTypeRef of a8 --> "union{intersection{union{Array<B#2>,Array<B#1>},union{intersection{J#2,J#2},B#3}},J#2}"
	let a8 : (([B#2<A#2>|B#1<A#1>])&((J#2<I#1, I#2>&J#2<A#1, A#2>|B#3<B#3<A#3>>)))|J#2<I#1, I#2>
}