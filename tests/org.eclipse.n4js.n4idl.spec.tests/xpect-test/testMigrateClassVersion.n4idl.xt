@Migration
function migration(a1: A # 1) : A # 2
{
	let a2 = new A#2();

	// XPECT type of 'a2' --> A#2
	// XPECT version of 'C' --> 2
	a2.c = new C#2();
	// XPECT type of 'a1' --> A#1
	a2.c.renamedI = a1.i;

	return a2;
}

class A # 1 {
	i: int;
	foo() { }
}

class A # 2 {
	// XPECT version of 'C' --> 2
	c: C;
	foo() { }
	bar() { }
}

class C # 1 {
	i: int;
}

class C # 2 {
	renamedI : int;
}
