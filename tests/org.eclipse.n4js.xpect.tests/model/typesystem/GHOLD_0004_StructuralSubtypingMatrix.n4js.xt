}
// Tests for T <: ~Primitive
{
	{
		let p = 1
		// Primitive <: ~Primitive XPECT noerrors -->
		let i : ~int = p;
	}
	{
		let o = new Object();
		// Object <: ~Primitive XPECT noerrors -->
		let i : ~int = o;
	}
	{
		// N4Object <: ~Primitive XPECT noerrors -->
		let i : ~int = new N4Object();
	}
	{
		let s_p : ~int = 3
		// ~Primitive <: ~Primitive XPECT noerrors -->
		let p : ~string = s_p
	}
	{
		let o = {};
		// ~Object without additional members <: ~Primitive XPECT noerrors -->
		let i : ~int = o;
	}
	{
		// ~Object with additional members <: ~Primitive XPECT noerrors -->
		let i : ~int = { f: "" }
	}
	{
		let s_n4o : ~N4Object = new N4Object();
		// ~N4Object without additional  members <: ~Primitive XPECT noerrors -->
		let p : ~string = s_n4o;
	}
	{
		// ~N4Object with additional members <: ~Primitive XPECT noerrors -->
		let i : ~int = new SA();
	}
}
