///// checking behaviour of caught exception control flow.
function f5(): number {
	console.log("a");
	try {

		try {

			var b : boolean = true;

			do {
				b = false;
			} while ( b )

			throw "Error"

		} catch ( e ) {

		}

		return 5;

	} catch ( e2 ) {

	}

	return 6;

	// Dead code
	// XPECT warnings --> "Dead code. No execution possible after return."  at "console.log("b");"
	console.log("b");

}


