// XPECT noerrors --> no missing return or throw statement
function f0() {
	var b1 : boolean = true;
    x: y: do {
		if ( b1 ) {
	       break x;

	    } else {
	       continue y;
		}

	} while(false)
}

// Dead Code after break
// XPECT noerrors --> no missing return or throw statement
function f0a() {
	var b1 : boolean = true;
    x: y: do {
		if ( b1 )
		{
	       break x;
	       // XPECT warnings --> "Dead code. No execution possible after break." at "f0();"
	       f0(); // Dead

	    } else {
	       continue y;
		}

	} while(false)
}


