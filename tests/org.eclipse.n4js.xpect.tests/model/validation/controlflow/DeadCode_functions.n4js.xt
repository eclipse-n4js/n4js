// XPECT noerrors --> no missing return or throw statement
function f1() {

   var b1 : boolean = true;

   y: do  {
       x: do  {
           if ( b1 )
           {
               break x;
               // XPECT warnings --> "Dead code. No execution possible after break." at "f0();"
         	   f0();	// Dead
           } else {
               break y;
           }
       // XPECT warnings --> "Dead code." at "true ) return; f0();"
       } while ( true )
       return;
       f0();  // Dead

   // XPECT warnings --> "Dead code." at "true"
   } while ( true )

}


