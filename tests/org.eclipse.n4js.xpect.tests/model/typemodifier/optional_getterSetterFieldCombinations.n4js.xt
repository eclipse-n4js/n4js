/* XPECT_SETUP org.eclipse.n4js.xpect.tests.N4JSXpectTest END_SETUP */


class CG {
	public get f?(): string {return null;}
}
class CS {
	public set f?(value:string) {}
}
class CGS {
	public get f?(): string {return null;}
	public set f?(value:string) {}
}
class CF {
	public f?: string;
}
class CGSMG { // MG is for "mandatory getter"
	public get f (): string {return null;}
	public set f?(value:string) {}
}
class CGSMS { // MS is for "mandatory setter"
	public get f?(): string {return null;}
	public set f (value:string) {}
}


@Final class D0 {
}
@Final class DG {
	public get f(): string {return null;}
}
@Final class DS {
	public set f(value:string) {}
}
@Final class DGS {
	public get f(): string {return null;}
	public set f(value:string) {}
}
@Final class DF {
	public f: string;
}


