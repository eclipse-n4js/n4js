class C {
  /* XPECT findReferences at field ---
      referenceFinder/destructuring/C_singlename - c.fieldA - 33,
      referenceFinder/destructuring/C_singlename - fieldA - 39
  ---*/
  fieldA: string = "hallo";

  /* XPECT findReferences at fieldB ---
      referenceFinder/destructuring/C_singlename - c.fieldB - 34,
      referenceFinder/destructuring/C_singlename - { fieldB } - 49
  ---*/
  fieldB: string = "hallo";
}

const c = new C();
c.fieldA;
c.fieldB;

/* XPECT findReferences at fieldA ---
	referenceFinder/destructuring/C_singlename - fieldA - 41
---*/
const { fieldA } = c; // this reference to 'field' is not found

fieldA;


/* XPECT findReferences at fieldB ---
	referenceFinder/destructuring/C_singlename - fieldB - 49,
	referenceFinder/destructuring/C_singlename - fieldB - 50
---*/
let fieldB;
({ fieldB } = c); // this reference to 'field' is not found
fieldB;
