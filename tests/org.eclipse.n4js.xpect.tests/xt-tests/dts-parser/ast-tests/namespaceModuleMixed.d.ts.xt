    N4ModuleDeclaration: actualModuleInNamespace
  ExportDeclaration: false, false
   N4NamespaceDeclaration: [public], myLegacyModule
    N4NamespaceDeclaration: [private], namespaceInLegacyModule
    ExportDeclaration: false, false
     N4NamespaceDeclaration: [public], namespaceInLegacyModuleExported
    N4ModuleDeclaration: actualModuleInLegacyModule
  N4NamespaceDeclaration: "myActualModule"
   N4NamespaceDeclaration: [private], namespaceInActualModule
