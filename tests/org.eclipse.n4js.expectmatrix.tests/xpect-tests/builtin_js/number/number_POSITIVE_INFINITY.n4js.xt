// Number.POSITIVE_INFINITY
// Source: https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/Number/POSITIVE_INFINITY

console.log(3*Number.POSITIVE_INFINITY);
console.log(Number.POSITIVE_INFINITY*Number.POSITIVE_INFINITY);
console.log(-3*Number.POSITIVE_INFINITY);
console.log(Number.NEGATIVE_INFINITY*Number.POSITIVE_INFINITY);
console.log(0*Number.POSITIVE_INFINITY);
console.log(NaN*Number.POSITIVE_INFINITY);
console.log(Number.POSITIVE_INFINITY/-3);
console.log(Number.POSITIVE_INFINITY/3);
console.log(Number.POSITIVE_INFINITY/Number.POSITIVE_INFINITY);
console.log(Number.POSITIVE_INFINITY/Number.NEGATIVE_INFINITY);
console.log(5/Number.POSITIVE_INFINITY);

