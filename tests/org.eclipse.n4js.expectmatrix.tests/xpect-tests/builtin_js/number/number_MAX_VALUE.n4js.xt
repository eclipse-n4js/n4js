/* XPECT_SETUP org.eclipse.n4js.expectmatrix.tests.N4JSExpectMatrixTest END_SETUP */

// Number.MAX_VALUE
// Source: https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/Number/MAX_VALUE

function f (num1 : number, num2 : number) {
	if (num1 * num2 <= Number.MAX_VALUE) {
   		console.log(num1 * num2);
	} else {
   		console.log("Number too big");
	}
}

f(675, 7826);
f(1.79E+308, 4);

/* XPECT output ---
5282550
Number too big
--- */
