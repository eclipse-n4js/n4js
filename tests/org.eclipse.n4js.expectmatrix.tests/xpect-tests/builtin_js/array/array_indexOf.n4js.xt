/* XPECT_SETUP org.eclipse.n4js.expectmatrix.tests.N4JSExpectMatrixTest END_SETUP */

// Array.prototype.indexOf()
// Source: https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/Array/indexOf

var array = [2, 5, 9];

console.log(array.indexOf(2));
console.log(array.indexOf(7));
console.log(array.indexOf(9, 2));
console.log(array.indexOf(2, -1));
console.log(array.indexOf(2, -3));

// Finding all occurrences of the element
var indices : Array<number> = [];
var array2 = ["a", "b", "a", "c", "a", "d"];
var idx = array2.indexOf("a");
while (idx != -1) {
	indices.push(idx);
	idx = array2.indexOf("a", idx + 1);
}
console.log(indices);

/* XPECT output ---
0
-1
2
-1
0
[ 0, 2, 4 ]
--- */
