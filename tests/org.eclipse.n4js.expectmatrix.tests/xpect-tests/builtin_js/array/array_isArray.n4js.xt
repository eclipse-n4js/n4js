/* XPECT_SETUP org.eclipse.n4js.expectmatrix.tests.N4JSExpectMatrixPendingTest END_SETUP */

// Array.isArray()
// Source: https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/Array/isArray

// all following calls return true
console.log("true = " + Array.isArray([]));
console.log("true = " + Array.isArray([1]));
console.log("true = " + Array.isArray( new Array<number>()));

// all following calls return false
console.log("false = " + Array.isArray({}));
console.log("false = " + Array.isArray(null));
console.log("false = " + Array.isArray(undefined));

/* XPECT output ---
true = true
true = true
true = true
false = false
false = false
false = false
--- */
