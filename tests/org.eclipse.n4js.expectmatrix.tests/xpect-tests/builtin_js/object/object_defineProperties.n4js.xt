/* XPECT_SETUP org.eclipse.n4js.expectmatrix.tests.N4JSExpectMatrixTest END_SETUP */

// Object.defineProperties()
// Source: https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/Object/defineProperties

var obj : Object+ = new Object();

Object.defineProperties(obj, {
  "property1": {
    value: true,
    writable: true,
    enumerable: true
  },
  "property2": {
    value: "Hello",
    writable: false,
    enumerable: true
  }
});

console.log(JSON.stringify(obj));

/* XPECT output ---
{"property1":true,"property2":"Hello"}
--- */
