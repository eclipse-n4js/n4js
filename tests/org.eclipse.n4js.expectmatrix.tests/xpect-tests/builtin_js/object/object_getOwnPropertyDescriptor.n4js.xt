/* XPECT_SETUP org.eclipse.n4js.expectmatrix.tests.N4JSExpectMatrixPendingTest END_SETUP */

// Object.getOwnPropertyDescriptor()
// Source: https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/getOwnPropertyDescriptor

var o : Object = { get foo() { return 17; } };
var d = Object.getOwnPropertyDescriptor(o, "foo");
console.log(d);

o = { bar: 42 };
d = Object.getOwnPropertyDescriptor(o, "bar");
console.log(d);

o = {};
Object.defineProperty(o, "baz", { value: 8675309, writable: false, enumerable: false });
d = Object.getOwnPropertyDescriptor(o, "baz");
console.log(d);

/* XPECT output ---
{
  get: [Function: get foo],
  set: undefined,
  enumerable: true,
  configurable: true
}
{ value: 42, writable: true, enumerable: true, configurable: true }
{
  value: 8675309,
  writable: false,
  enumerable: false,
  configurable: false
}
--- */
