/* XPECT_SETUP org.eclipse.n4js.expectmatrix.tests.N4JSExpectMatrixPendingTest END_SETUP */

// Math.asin()
// Source: https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/Math/asin

console.log(Math.asin(-2));  // NaN
console.log(Math.asin(-1).toString().substring(0,12));  // -1.570796326 (-pi/2)
console.log(Math.asin(0));   // 0
console.log(Math.asin(0.5).toString().substring(0,12)); // 0.5235987755
console.log(Math.asin(1).toString().substring(0,12));   // 1.5707963267 (pi/2)
console.log(Math.asin(2));   // NaN

/* XPECT output ---
NaN
-1.570796326
0
0.5235987755
1.5707963267
NaN
--- */
