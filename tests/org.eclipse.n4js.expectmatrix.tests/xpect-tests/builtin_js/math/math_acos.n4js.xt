/* XPECT_SETUP org.eclipse.n4js.expectmatrix.tests.N4JSExpectMatrixPendingTest END_SETUP */

// Math.acos()
// Source: https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/Math/acos

console.log(Math.acos(-2));  // NaN
console.log(Math.acos(-1).toString().substring(0,12));  // 3.1415926535
console.log(Math.acos(0).toString().substring(0,12));   // 1.5707963267
console.log(Math.acos(0.5).toString().substring(0,12)); // 1.0471975511
console.log(Math.acos(1));   // 0
console.log(Math.acos(2));   // NaN

