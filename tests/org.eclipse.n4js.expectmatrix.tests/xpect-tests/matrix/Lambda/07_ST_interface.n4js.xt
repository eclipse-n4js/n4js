/* XPECT_SETUP org.eclipse.n4js.expectmatrix.tests.N4JSExpectMatrixTest END_SETUP  */

class S {
	public num : number;
	public str : string;
}

//////////////
// ST		//
// Role		//
//////////////

interface B {
	public s : ~S;

	public bar () {
		// setting ST inside a lambda expr. XPECT noerrors -->
		(() => { this.s = { num: 5, str: "S" } })();
	}

	public foo () {
		// using parameters XPECT noerrors -->
		((n : number, m : string) => { this.s = { num: n, str: m } })(4, "D");
	}

	public baz() : void {
		// expecting ST XPECT noerrors -->
		((st : ~S) => { this.s = st })({ num: 5, str: "S" });
	}
}
