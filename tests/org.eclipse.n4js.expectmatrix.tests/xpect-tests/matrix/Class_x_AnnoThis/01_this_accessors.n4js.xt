// IDE-1010, IDE-2313
class A {
	// XPECT noerrors -->
	@This(B)
	get acc() : string {
		if (this.s) {
			return this.s;
		}
		return "";
	}
	// XPECT noerrors -->
	@This(B)
	set acc(n : string) { this.s = n; }
}
class B extends A {
	public s: string;
}
