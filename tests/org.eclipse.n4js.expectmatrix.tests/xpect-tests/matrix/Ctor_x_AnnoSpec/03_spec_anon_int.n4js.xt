class A {
	public x : number;
	protected y : number;
	private z :  number;
	w : number;

	public constructor (@Spec spec : ~i~this with {y: number; z: number; }) {
		this.y = spec.y;
	}
}

// all fields required by ctor present XPECT noerrors -->
var a : A = new A({x: 1, y: 2, z: 3});

