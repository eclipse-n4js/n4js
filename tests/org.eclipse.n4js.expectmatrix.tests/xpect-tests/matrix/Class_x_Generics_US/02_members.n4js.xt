/* XPECT_SETUP org.eclipse.n4js.expectmatrix.tests.N4JSExpectMatrixPendingTest END_SETUP  */

/////////////
// Members //
/////////////

class A<T> {
	a : T;

	f () : T { return this.a; };
	f2 () : A<T> { return this; };
	f3 () : {function() : T} {
		var self = this;
		var t = function () : T {return self.a;};
		return t;
	}
}

class B<T,S> {
	b : T;
	c : S;

	f (s : S) : S { return s;}
	f2 () : this { return this; }
}

class E<T> {
	e : A<T>;

	f2 () : this { return this; }
}

class C {}
class D<T> extends C { d : T; }

var a1 : A<string> = new A<string>();

// assigning T to T field XPECT errors --> "int is not a subtype of string." at "2"
a1.a = 2;

// calling the method returning A<T> XPECT errors --> "A<string> is not a subtype of number." at "a1.f2()"
var n1 : number = a1.f2();


var b1 : B<A<string>, A<number>> = new B<A<string>, A<number>>();

// accessing fields of the field XPECT errors --> "int is not a subtype of string." at "4"
b1.b.a = 4;

// calling the method returning this XPECT errors --> "B<A<string>, A<number>> is not a subtype of number." at "b1.f2()"
var n2 : number = b1.f2();

// A more complicated example

var b2 : B<A<C>, A<D<string>>> = new B<A<C>, A<D<string>>>();

// accessing fields of the parameterized field XPECT noerrors -->
b2.b.a = new D<string>();

// accessing fields of the field 2 levels deep XPECT errors --> "int is not a subtype of string." at "2"
b2.c.a.d = 2;

var e1 = new E<string>();

// XPECT warnings --> "A reference to method f2 is created detached from a (correct) this-instance." at "f2"
// A<T> inside E<T> XPECT type of e2 --> {function():A<string>}
var e2 = e1.e.f2;
