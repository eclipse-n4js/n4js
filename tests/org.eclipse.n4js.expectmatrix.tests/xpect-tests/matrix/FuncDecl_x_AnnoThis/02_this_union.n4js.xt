/* XPECT_SETUP org.eclipse.n4js.expectmatrix.tests.N4JSExpectMatrixTest END_SETUP  */

//////////////////////////////////
// union{} as argument of @This //
// Example 66				    //
//////////////////////////////////

class A {
	log() : string { return "A was logged"; }
}
class B {
	log() : string { return "B was logged"; }
}
class C {
	log() : string { return "C was logged"; }
}

@This(union{A,B})
function logger() { console.log("~" + this.log() + "~"); }

var a : A = new A();
var b : B = new B();
var c : C = new C();

logger.call(a);
logger.call(b);
logger.call(c);

/* XPECT output ---
~A was logged~
~B was logged~
~C was logged~
--- */
