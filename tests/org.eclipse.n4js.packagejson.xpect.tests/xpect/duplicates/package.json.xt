/* XPECT_SETUP org.eclipse.n4js.packagejson.xpect.tests.PackageJsonXpectTest END_SETUP */
{
	"name": "duplicates",
	"version": "0.0.1-SNAPSHOT",
	// XPECT errors --> "There is no output folder defined, so compilation isn't possible." at ""n4js""
	"n4js": {
		"projectType": "library",
		"projectVersion": "0.0.1-SNAPSHOT",
		"vendorId": "MyVendorID",
		"vendorName": "My Vendor Name",
		"sources": {
			"source": [
				// XPECT warnings --> "Duplicate path "src" has already been declared as source container in external." at "\"src\""
				"src",
				// IDEBUG-339 XPECT warnings --> "Source container path src_does_not_exist does not exist." at ""src_does_not_exist""
				"src_does_not_exist",
				// XPECT errors --> ""src/p/juergensHack/*.js" is not a valid path." at ""src/p/juergensHack/*.js""
				"src/p/juergensHack/*.js",
				// XPECT errors --> "Path "src/p/myAlreadyAsModuleHack.js" does not point to a directory." at ""src/p/myAlreadyAsModuleHack.js""
				"src/p/myAlreadyAsModuleHack.js",
				"src2",
				"src3"
			],
			"external": [
				// XPECT warnings --> "Duplicate path "src" has already been declared as source container in source." at "\"src\""
				"src",
				"src-external"
			]
		},
		"moduleFilters": {
			"noValidate": [
				// TODO re-enable the following assertions
				// X!PECT errors --> "Module specifier p/wolfgangsUglyHack.js doesn't exist." at "\"p/wolfgangsUglyHack.js\""
				"p/wolfgangsUglyHack.js",
				// X!PECT errors --> "Module specifier p/wolfgangsUglyHack.* doesn't exist." at "\"p/wolfgangsUglyHack.*\""
				"p/wolfgangsUglyHack.*",
				"p/wolfgangsUglyHack",
				"p/juergensHacks/*",
				// X!PECT errors --> "Duplicate module specifier." at "\"p/juergensHacks/*\""
				"p/juergensHacks/*",
				// X!PECT errors --> "Module specifier p/doesntexist/ doesn't exist." at "\"p/doesntexist/\""
				"p/doesntexist/",
				{
					// X!PECT errors --> "Module specifier **/External doesn't exist." at "\"**/External\""
					"src": "**/External"
				},
				{
					// External is available in src-external but not in src
					"src-external": "p/External"
				},
				// X!PECT errors --> "\"***\" isn't a valid character sequence in a wild card." at "\"***/A\""
				"***/A",
				// X!PECT errors --> "Relative navigation isn't allowed in a module specifier." at "\"**/../p/A\""
				"**/../p/A",
				// X!PECT errors --> "noValidate paths shouldn't match n4js files." at "\"other/*\""
				"other/*",
				{
					"src2": "**/*"
				},
				{
					"src3": "**/*"
				}
			],
			// XPECT warnings --> "Property noValidate duplicates property noValidate (line 42)." at "\"noValidate\""
			"noValidate": [
				"p/A"
			],
			"noModuleWrap": [
				"p/myAlreadyAsModuleHack",
				// X!PECT errors --> "noModuleWrap paths shouldn't match n4js files." at "\"other/*\""
				"other/*"
			]
		},
		"requiredRuntimeLibraries": [
			"vendorA:libA",
			// N4JS validation XPECT noerrors -->"Duplicate runtime library MyVendorID:libB." at "libB"
			"libB",
			"vendorB:libA",
			// N4JS validation XPECT noerrors -->"Duplicate runtime library MyVendorID:libA." at "libA"
			"libA",
			// N4JS validation XPECT noerrors -->"Duplicate runtime library MyVendorID:libA." at "MyVendorID:libA"
			"MyVendorID:libA",
			// N4JS validation XPECT noerrors -->"Duplicate runtime library MyVendorID:libB." at "libB"
			"libB",
			"MyVendorID:libC"
		]
	}
}
