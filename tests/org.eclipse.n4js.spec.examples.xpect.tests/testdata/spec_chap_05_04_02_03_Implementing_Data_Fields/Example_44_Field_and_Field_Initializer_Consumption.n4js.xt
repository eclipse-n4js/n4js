interface I0 {
	v : string = "I0";
}

interface I1 {
	s : string = "I1";
	t : string = "I1";
	u : string = "I1";
}

interface I2 extends I1, I0 {
	@Override
	t : string = "I2";
	@Override
	v : string = "I2";
}

class C {
	s : string = "C";
}

class D extends C implements I1, I2 {
	@Override
	t : string = "D";
}

var d = new D();

console.log(
	"s:", d.s, ", t:", d.t, ",u:", d.u,	",v:", d.v
)
