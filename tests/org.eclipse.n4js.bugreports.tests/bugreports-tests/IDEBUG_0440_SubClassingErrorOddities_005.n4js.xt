/* XPECT_SETUP org.eclipse.n4js.bugreports.tests.N4JSBugreportTest END_SETUP  */

/* XPECT output ---
e.message === some message: true
e instanceof Error: true
e instanceof CustomError_005: true
e instanceof SubCustomError_005: true
e has stack trace: true
e.stack starts with 'SubCustomError_005: some message': true
--- */

class CustomError_005 extends Error {
}

class SubCustomError_005 extends CustomError_005 {
	constructor(someMsg : string) {
		super(someMsg);
	}
}

var e = new SubCustomError_005('some message');
console.log('e.message === some message:', e.message === 'some message');
console.log('e instanceof Error:', e instanceof Error);
console.log('e instanceof CustomError_005:', e instanceof CustomError_005);
console.log('e instanceof SubCustomError_005:', e instanceof SubCustomError_005);
// XPECT warnings --> "Unnecessary cast from SubCustomError_005 to Object" at "e as Object"
var stack = (e as Object)['stack'];
console.log('e has stack trace:', undefined !== stack);
console.log('e.stack starts with \'SubCustomError_005: some message\':', 0 === (stack as String).indexOf('SubCustomError_005: some message'));
