/* XPECT_SETUP org.eclipse.n4js.bugreports.tests.N4JSBugreportTest END_SETUP  */

// ~number is equivalent to any thus we can simply assign an empty object literal XPECT noerrors 
let a : ~number = {};
// No auto-boxing for structural primitive types XPECT errors --> "Couldn't resolve reference to IdentifiableElement 'toFixed'." at "toFixed"
console.log(a.toFixed(3));

let a1 : number = 1
// Auto-boxing for nominal primitive types should work as expected XPECT noerrors
console.log(a1.toFixed(3));

let num : ~Number = new Number(3);
// Using the auto-boxing types directly should work as expected XPECT noerrors
console.log(num.toFixed(3));

let t1 : ~string = new Object();
let t2 : ~string = 3;
let t3 : ~number = 4;
let t4 : any = t1 //~string
let t5 : ~string = t3 // ~number