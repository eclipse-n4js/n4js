/* XPECT_SETUP org.eclipse.n4js.transpiler.es5.xpect.tests.CompileAndExecuteTest END_SETUP */

/* XPECT output ---
B#foo: msg
A#foo: msg-delegate
--- */

class A {
	foo(s : string) {
		console.log('A#foo: ' + s)
	}
}

class B extends A {
	@Override
	foo(s : string) {
		console.log('B#foo: ' + s)
		super.foo(s+'-delegate')
	}
}

new B().foo('msg');
