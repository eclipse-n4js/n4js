class Foo {
	private innerMe : string = "me";

	constructor() {
    	console.log("constructor")
	};

    bar() : void {
    	console.log("bar")
    };

	bar2() : void {
    	console.log("bar2")
    };

    get me() : string {
    	console.log("get me")
    	return this.innerMe;
    };

	get me2() : string {
    	console.log("get me2")
    	return this.innerMe;
    };

    set me(me : string) {
    	console.log("set me")
    	this.innerMe = me;
    };

	set me2(me : string) {
    	console.log("set me2")
    	this.innerMe = me;
    };
}

var foo = new Foo()
foo.bar();
foo.bar2();
foo.me = "me, too"
foo.me2 = "me2, too"
console.log(foo.me)
console.log(foo.me2)
