/* XPECT output ---
hi
hi
hi
hi
hi
hi
hi
hi
hi
hi
--- */
class A{
	/** private say*/
	private say(s : string) : void{console.log(s)}
	/** repeat say n times (client must define its own say) */
	protected repeat(n : number, s : string) : void{
		for (var i = 0; i < n; i++) {
			this.say(s);
		}
	}
	/*public api*/
	public talk(){
		this.repeat(10, 'hi');
	}
}
var a = new A();
a.talk();
