/* XPECT output ---
- F: fixed, variadic -
Fixed: s1
Variadic length: 0
Fixed: s1
Variadic length: 2
Variadic: s2
Variadic: s3
- subclass of F -
Fixed: s1
Variadic length: 0
Fixed: s1
Variadic length: 2
Variadic: s2
Variadic: s3
--- */
class F {
	constructor(fixed : string, ... s : string) {
		console.log("Fixed: " + fixed)
		console.log("Variadic length: " + s.length)
		for(var i=0, l = s.length; i < l; i++){
			console.log("Variadic: " + s[i])
		}
	}
}
class F2 extends F {}

console.log("- F: fixed, variadic -")
new F("s1")
new F("s1", "s2", "s3")
console.log("- subclass of F -")
new F2("s1")
new F2("s1", "s2", "s3")
