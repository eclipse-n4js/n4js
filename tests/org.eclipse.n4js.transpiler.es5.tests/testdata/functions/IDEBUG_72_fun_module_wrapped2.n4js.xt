function safeEval(dyn){
	try{
		eval(dyn)
	} catch(e){
		console.log('error when running ' + dyn + ' : ' + e);
	}
}

(function(){
	safeEval('logFactors(-100)');
	safeEval('logFactors(-10)');
	safeEval('logFactors(-1)');
	safeEval('logFactors(0)');
	safeEval('logFactors(1)');
	safeEval('logFactors(10)');
	safeEval('logFactors(100)');
})();
