/* XPECT output ---
f( x , undefined , [] )
f( x , 42 , [] )
f( x , 42 , [ 1 ] )
f( x , 42 , [ 1, 2 ] )
f( x , 42 , [ 1, 2, 3, 4, 5 ] )
a.f( x undefined [] )
a.f( x 42 [] )
a.f( x 42 [ 1 ] )
a.f( x 42 [ 1, 2 ] )
a.f( x 42 [ 1, 2, 3, 4, 5 ] )
--- */

function f(s : string, n : number=undefined, ... args : number) {
	console.log("f(", s, ",", n, ",", args, ")");
}

class A {
	f(s : string, n : number=undefined, ... args : number) {
		console.log("a.f(", s, n, args, ")");
	}
}

f("x")
f("x", 42)
f("x", 42, 1)
f("x", 42, 1, 2)
f("x", 42, 1, 2, 3, 4, 5)

var a = new A();
a.f("x")
a.f("x", 42)
a.f("x", 42, 1)
a.f("x", 42, 1, 2)
a.f("x", 42, 1, 2, 3, 4, 5)
