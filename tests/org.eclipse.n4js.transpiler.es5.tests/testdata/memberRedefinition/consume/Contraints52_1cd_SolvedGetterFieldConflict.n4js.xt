interface R1 {
	x : string = "R1.x";
}
interface R2 {
	set x(s : string) { console.log("R2.x set: " + s) }
}


class C1 implements R1, R2 {
	_x : string = "C1._x";
	@Override set x(s : string) { console.log("C1.x set: " + s); this._x = s}
	@Override get x() : string { console.log("C1.x get"); return this._x}
}

var c = new C1();
console.log(c.x)
c.x = "newvalue";
console.log(c.x)
