/* XPECT output ---
C.s set: S.s
C.s get
C._s
C.s set: newValue
C.s get
newValue
--- */

class S {
	public s : string = "S.s";
}

class C extends S {
	_s : string = "C._s";
	@Override public get s() : string { console.log("C.s get"); return this._s}
	@Override public  set s(p : string) { console.log("C.s set: " + p); this._s = p}
}

var c : C = new C();
console.log(c.s);
c.s = "newValue";
console.log(c.s);
