	foo(n : number, m : number=undefined) {
		console.log("interface method foo: " + n + ", " + m)
	}
}

class Class2 implements Role1 {

}

var ol = {
	foo : function foo(n : number, m : number=undefined) {
		console.log("object literal function foo: " + n + ", " + m)
	}
}

foo(1, 2)
