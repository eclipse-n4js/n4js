class C {

  z : number;

  m1(x : number) : {function(number):number} {
	return y => x * y * this.z;
  }

  m2(x : number) : {function(number):number} {
	var self = this
	return function(y) {
  		return x * y * self.z
    }
  }

}

var c = new C
c.z = 3

var times15viaArrow = c.m1(5)
var times15viaTraditional = c.m2(5)

console.log(times15viaArrow(9))
console.log(times15viaTraditional(9))
