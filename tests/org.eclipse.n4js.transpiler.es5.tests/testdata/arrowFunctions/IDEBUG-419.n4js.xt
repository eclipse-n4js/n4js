class C {

	private _data : number = -1;

	argsTotal : number = 0;

	public get data() {
  		if (this._data==-1) {
  			(() => { this._data = 1; })()
		}
		(()=>{ this.argsTotal += arguments.length })()
		return this._data;
	}

	public set data(data : number) {
		(() => { this._data = data; })();
		(()=>{ this.argsTotal += arguments.length })()
		this.notifyListeners();
	}

	notifyListeners() : void {
	}

}

var c = new C
console.log("args total " + c.argsTotal)
console.log("data " + c.data)
c.data = 2
console.log("args total " + c.argsTotal)
console.log("data " + c.data)
c.data = 3
console.log("args total " + c.argsTotal)
console.log("data " + c.data)
