function <S extends string, T, P> wrap() : {@This(any)function(S,T,P,any,number,string):number} {
	@This(any)
	function len(s : S, t : T,p : P, a, n : number=undefined, ... vas : string) : number {
		return (s+t+p+a+n+vas).length
	}
	return len;
}

