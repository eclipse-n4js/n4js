// IDE-1805  transpiler changes for computed-names of string-literal form in member declarations

/* XPECT output ---
C1 {
  'm@12@': '1',
  '@abc': '3',
  '@abc1': '4',
  '@abc2': '5',
  '@abc3': '6'
}
--- */

class C1 {

	['m@12@'] : string = "1";

	["@abc"] : string = "3";
	[    '@abc1'    ] : string = "4";
	[ /*comment*/ '@abc2'] : string = "5";
	['@abc3'  /*comment*/ ] : string = "6";

}

var c = new C1

console.log(c)
