/* XPECT_SETUP org.eclipse.n4js.transpiler.es5.xpect.tests.CompileAndExecuteTest END_SETUP */


/* XPECT output ---
<==
stdout:
========================================
I#m()
I#g()
I#s(), p: hello setter!
I#m()
I#g()
I#s(), p: hello setter!
========================================
stderr:
==>
--- */



interface I {
	m() {
		console.log('I#m()');
	}
	get g() {
		console.log('I#g()');
		return null;
	}
	set s(p) {
		console.log('I#s(), p:', p);
	}
}
interface J extends I {}
interface K extends J {}



console.log('========================================');

// assert that a delegate to I#m() was added to J
var j : any+ = J;
Object.getOwnPropertyDescriptor(j.$defaultMembers as Object, 'm')['value']();
Object.getOwnPropertyDescriptor(j.$defaultMembers as Object, 'g')['get']();
Object.getOwnPropertyDescriptor(j.$defaultMembers as Object, 's')['set']('hello setter!');

// assert that a delegate to I#m() was added to K
var k : any+ = K;
Object.getOwnPropertyDescriptor(k.$defaultMembers as Object, 'm')['value']();
Object.getOwnPropertyDescriptor(k.$defaultMembers as Object, 'g')['get']();
Object.getOwnPropertyDescriptor(k.$defaultMembers as Object, 's')['set']('hello setter!');

console.log('========================================');
