interface I1 {
    public set y(y : int);
}

interface I2 {
    @ProvidesInitializer
    public set y(y : int);
}

class C1 implements I1 {
    @ProvidesInitializer
    @Override
    public set y(y : int) { }

    public constructor(@Spec spec : ~i~this) { }
}

class C2 extends C1 implements I2 {
    @Override
    public set y(y : int) { }
}

class C3 extends C2 implements I2 {
    @ProvidesInitializer
    @Override
    public set y(y : int) { }
}

// XPECT noerrors --> ""
new C1({});
// XPECT noerrors --> ""
new C1({y : 36});

// XPECT errors --> "~Object is not a structural subtype of ~i~C2: missing getter y; y failed: non-optional writable field requires a readable field or a getter in subtype." at "{}"
new C2({});
// XPECT noerrors --> ""
new C2({y : 36});

// XPECT noerrors --> ""
new C3({});
// XPECT noerrors --> ""
new C3({y : 36});
