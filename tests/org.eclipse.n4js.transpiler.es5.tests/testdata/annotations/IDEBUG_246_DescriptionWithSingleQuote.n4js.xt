	console.log("@", a.name)
	a.details.forEach(function(ad){
		console.log(" ", "t1", ad)
		console.log(" ", "t2",ad==="<'>")
		console.log(" ", "t3",JSON.stringify(ad))
		console.log(" ", "t4",ad.length, "<'>".length)
	});
});


