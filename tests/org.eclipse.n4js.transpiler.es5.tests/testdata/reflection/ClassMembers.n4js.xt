    console.log(label, members.map((m)=>m.name).join(', '));
}

class C {
    field;
    fieldD = 1;
    static sField;
    static sFieldD = 2;
    static get sGetterD() {return null;}
    static set sSetterD(a) { }
    get getterD() {return null;}
    set setterD(a) { }
    fooD() {}
    static sFooD() {}
}




show("members of C:", C.n4type.members(true,true,true));

console.log(C.n4type);


