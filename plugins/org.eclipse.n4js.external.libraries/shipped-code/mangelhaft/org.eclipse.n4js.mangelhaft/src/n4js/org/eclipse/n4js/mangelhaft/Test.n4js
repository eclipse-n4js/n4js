export public function async(cb: Function, reject: Function): Function {
    return function() {
        try {
            cb.apply(null, arguments as Array<?>);
        } catch(ex) {
            try {
                reject.call({}, ex);
            } catch (ex3) {
                console.log(ex3);
            }
        }
    }
}

